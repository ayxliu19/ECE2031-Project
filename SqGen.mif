-- Altera Memory Initialization File (MIF)

DEPTH = 2048;
WIDTH = 16;

ADDRESS_RADIX = HEX;
DATA_RADIX = HEX;

CONTENT
  BEGIN
    [000..7FF] : 0000;  -- Default to NOP

           000 : 9000;  --   in Switches
           001 : 6002;  --   shift 2
           002 : 9820;  --   OUT  MusicGen
           003 : 2800;  -- 	jump Loop
  END;
